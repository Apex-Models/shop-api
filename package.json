{
  "name": "shop-api",
  "version": "0.0.1",
  "description": "",
  "homepage": "https://github.com/Apex-Models/shop-api#readme",
  "bugs": {
    "url": "https://github.com/Apex-Models/shop-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apex-Models/shop-api.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "src/app.ts",
  "scripts": {
    "dev": "nodemon --watch src --exec tsx src/app.ts",
    "start": "node dist/app.js",
    "build": "babel src --out-dir dist --extensions \".ts,.js\" --copy-files --no-comments",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "typecheck:strict": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "lint-staged": "lint-staged",
    "format": "prettier --check \"src/**/*.{js,ts}\"",
    "format:write": "prettier --write \"src/**/*.{js,ts}\"",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^22.16.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.8.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@prisma/client": "^6.8.2",
    "axios": "^1.10.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3"
  }
}
